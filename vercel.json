{
  "version": 2,
  "builds": [
    {
      "src": "myproject/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/index.html$",
      "dest": "/templates/index.html"
    },
    {
      "src": "/about.html$",
      "dest": "/templates/about.html"
    },
    {
      "src": "/contact.html$",
      "dest": "/templates/contact.html"
    },
    {
      "src": "/home.html$",
      "dest": "/templates/home.html"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "myproject/wsgi.py"
    }
  ]
}